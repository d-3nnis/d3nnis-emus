{
  "name": "frontend",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
      "build-wasm": "cd ../emuweb-wasm && wasm-pack build --target web",
      "watch-wasm": "nodemon --watch ../emuweb-wasm/src --ext rs --exec \"npm run build-wasm\"",
      "dev": "npm run build-wasm && concurrently \"npm run watch-wasm\" \"vite\"",
      "start": "npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "emuweb": "file:../emuweb-wasm/pkg"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "vite": "^6.3.5"
  }
}
